 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
      Nand(a=a, b=a, out=i1);
      Nand(a=b, b=b, out=i2);
      Nand(a=i1, b=i2, out=out);
}
